{
  "name": "llama-gpu-dashboard",
  "version": "1.0.0",
  "description": "React-based monitoring dashboard for LLaMA GPU inference platform",
  "main": "public/index.html",
  "homepage": ".",
  "scripts": {
    "start": "../scripts/start.sh",
    "start:react": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "mock-server": "cd .. && python3 -m uvicorn mock_api_server:app --reload --port 8000 --host 0.0.0.0",
    "electron": "electron .",
    "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:${PORT:-3001} && electron .\"",
    "dist": "npm run build && electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hkevin01/Llama-GPU.git"
  },
  "author": "hkevin01",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.0",
    "@mui/material": "^5.14.0",
    "@mui/system": "^5.14.0",
    "@types/leaflet": "^1.9.20",
    "axios": "^1.4.0",
    "framer-motion": "^10.12.0",
    "leaflet": "^1.9.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.14.0",
    "react-scripts": "5.0.1",
    "recharts": "^2.7.2",
    "socket.io-client": "^4.7.0",
    "web-vitals": "^3.3.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.0",
    "@babel/preset-react": "^7.22.0",
    "@craco/craco": "^7.1.0",
    "@types/node": "^24.1.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "concurrently": "^8.2.0",
    "electron": "^25.0.0",
    "electron-builder": "^24.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.10.2",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "http-proxy-middleware": "^3.0.5",
    "prettier": "^3.0.0",
    "typescript": "^4.9.5",
    "wait-on": "^7.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.llama-gpu.dashboard",
    "productName": "LLaMA GPU Dashboard",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "public/electron.js"
    ]
  }
}
